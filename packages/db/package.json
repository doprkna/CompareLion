{
  "name": "@parel/db",
  "private": true,
  "version": "0.0.1",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "import": "./index.ts",
      "require": "./index.ts",
      "default": "./index.ts"
    },
    "./src/client": "./src/client.ts"
  },
  "prisma": {
    "schema": "schema.prisma",
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "seed": "tsx prisma/seed.ts",
    "generate": "prisma generate",
    "migrate:dev": "prisma migrate dev --name sync_schema",
    "postinstall": "prisma generate",
    "db:gen": "pnpm --filter @parel/db run generate",
    "db:migrate": "pnpm --filter @parel/db run migrate:dev"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "express": "^5.1.0",
    "jest": "^30.2.0",
    "prisma": "^5.16.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.5"
  }
}
