    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.17.0",
    "@upstash/redis": "^1.35.4",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^4.15.4",
