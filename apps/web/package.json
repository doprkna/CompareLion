{
  "name": "@parel/web",
  "version": "0.12.8",
  "private": true,
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=json --reporter=verbose --outputFile=test-results.json",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ci": "playwright test --reporter=json",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix && prettier --write .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "audit:structure": "tsx ../../scripts/audit-structure.ts",
    "analyze": "ANALYZE=true pnpm build",
    "gen:questions": "tsx scripts/generate-questions.ts"
  },
  "dependencies": {
